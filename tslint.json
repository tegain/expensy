{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-standard"
  ],
  "linterOptions": {
    "exclude": ["**/node_modules/**"]
  },
  "jsRules": {},
  "rules": {
    "trailing-comma": [ false ],
    "interface-name": [false],
    "no-console": [false],
    "quotemark": [true, "single"]
  },
  "rulesDirectory": []
}
